{
  "name": "chunked-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "web:dev": "cd apps/web && npm run dev",
    "mobile:dev": "cd apps/mobile && npm start",
    "dev": "concurrently \"npm run convex:dev\" \"npm run web:dev\"",
    "install:all": "npm install && cd apps/web && npm install && cd ../mobile && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "convex": "^1.16.1",
    "@convex-dev/auth": "^0.0.61"
  }
}
